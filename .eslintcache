neku vrstu array\r\n      snapshot.forEach( data => {\r\n        setSmoothie(data.val()); \r\n        setLoading(false);\r\n    });\r\n  }).catch(error => {\r\n    setLoading(false);\r\n    setShowModal(true);\r\n    setModalMsg(error.code);\r\n  })\r\n  }, [id, setShowModal, setModalMsg, englishLang]);\r\n\r\n  if (loading) {\r\n    return <Loading />\r\n  }\r\n\r\n  const {name, imageURL, ingredients,instructions, preparation, nutrition} = smoothie;\r\n\r\n  let updatedNutrition = [];\r\n  for (const key in nutrition) {\r\n      updatedNutrition.push(`${key}: ${nutrition[key]}`)\r\n  }\r\n\r\n  let helperClass = '';\r\n  if (likedList.map(item => item.id).includes(id)) {\r\n    helperClass = 'btn-like liked';\r\n  } else {\r\n    helperClass='btn-like';\r\n  }\r\n  return (\r\n    <section className='section smoothie-section'>\r\n      <Link to='/' className='btn btn-primary'>\r\n        {langOption.btn_back}\r\n      </Link>\r\n      <h2 className='section-title'>{name}</h2>\r\n      <div className='drink-info'>\r\n        <img src={`.${imageURL}`} alt={name} />\r\n\r\n        <ul className='drink-list'>\r\n\r\n          <li className='drink-item'>\r\n            <span className='drink-name'>{langOption.ingr}</span>\r\n            {ingredients.map((item, index) => {\r\n              return <p key={item}>\r\n                      {item}\r\n                    </p>\r\n            })}\r\n          </li>\r\n\r\n          <li className='drink-item'>\r\n            <span className='drink-name'>{langOption.instr}</span>\r\n            {instructions.split('. ').map((item, index) => {\r\n                return <p key={item}>\r\n                  <span className='ordinary-number'>{index + 1 + '.'}</span>\r\n                  {item + '.'}\r\n                </p>\r\n            })}\r\n          </li>\r\n\r\n          <li className='drink-item'>\r\n            <span className='drink-name'>{langOption.prep}</span>\r\n            {preparation}\r\n          </li>\r\n\r\n          <li className='drink-item'>\r\n            <span className='drink-name'>{langOption.nutr}</span>\r\n            <div className='grid-container'>\r\n              {updatedNutrition.map((item, index) => {\r\n                return <p key={index}>\r\n                          { item.includes('Calories') || item.includes('N/A') || item.includes('Serving Size') || item.includes('Kalorije')? item  : item + 'gr'}\r\n                        </p>\r\n              })}\r\n            </div>\r\n          </li>\r\n\r\n          <li>\r\n            <div className='grid-container mt-3'>\r\n              <p className='likes-text'>{langOption.fav_title}</p>\r\n              <button className={helperClass} onClick={() => toggleLike(smoothie)}>\r\n                <FiHeart className='nav-favorites-icon'/> \r\n              </button>\r\n            </div>\r\n\r\n          </li>\r\n\r\n        </ul>\r\n\r\n      </div>\r\n    </section>\r\n  )\r\n}\r\n\r\nexport default SingleSmoothie;","D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\pages\\LikedSmoothies.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\components\\Navbar.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\components\\Sidebar.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\firebaseConfig.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\assets\\utils\\pagination.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\components\\Smoothie.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\assets\\languages\\howToLang.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\components\\SearchForm.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\assets\\links.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\assets\\languages\\historyLang.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\assets\\languages\\aboutLang.js",[],"D:\\Ostalo\\vlada\\Tutorijali\\Projects\\smoothie\\src\\assets\\